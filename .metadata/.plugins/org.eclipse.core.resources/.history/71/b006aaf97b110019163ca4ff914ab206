package Arrays;

/**
 * 获取局部最大值
 * 
 * @author xubing
 *
 */
public class LocalMaxNum {

    public static int getLocalMaxNum(int[] arr) {
        int result = 0;
        int left = 0;
        int right = arr.length - 1;
        int mid = 0;

        while (left < right) {
            mid = (left + right) / 2;
        }


        return result;
    }

    public static void main(String[] args) {
        // TODO Auto-generated method stub

    }

}
