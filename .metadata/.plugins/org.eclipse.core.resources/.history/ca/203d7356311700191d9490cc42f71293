package Tree;

/**
 * 哈夫曼编码--前缀编码
 * 
 * @author xubing
 *
 */
public class HuffmanEnCode {


    public void calcFrequency(String data, char[] result) {
        // 字符都在0~255中
        for (int i = 0; i < data.length(); i++) {
            result[data.charAt(i)]++;
        }
    }

    public static void main(String[] args) {
        // TODO Auto-generated method stub

    }

}
