package String;

/**
 * 字符串左移K位
 * 
 * @author xubing
 *
 */
public class LeftRotateString {


    private static void rotateString(char[] str, int from, int to) {

        char tmp = ' ';
        while (from < to) {
            tmp = str[from];
            str[from++] = str[to];
            str[to--] = tmp;
        }
    }
    
    /**
     * 
     * @param str
     * @param n 字符串长度
     * @param index 指定移动位置
     */
    public static void leftRotateString(char[] str, int strLength, int leftIndex) {
        
    }

    public static void main(String[] args) {
        // TODO Auto-generated method stub

    }

}
